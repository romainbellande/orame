datasource db {
    provider = "postgres"
    url      = "postgresql://orame:orame@localhost:5432/orame"
}
 
generator client {
    provider      = "cargo prisma"
    output        = "../src/db.rs"
}
 
model User {
    id          String  @id
    name        String
    email       String  @unique
    password    String
}

